<FluentStack Orientation="Orientation.Vertical">
    <ForEach Source="Values">
        <Template>
            <Copyable Text="@context"
                      Inline="true"
                      Style="font-family:Consolas, monospace;line-height:1.4rem;word-break:break-all;" />
        </Template>
        <Separator>
            <FluentDivider Style="width: 100%;" Role="DividerRole.Separator" />
        </Separator>
    </ForEach>
</FluentStack>
